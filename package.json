{
  "name": "bhesi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "backend:dev": "ts-node --project backend/tsconfig.backend.json backend/src/server.ts",
    "backend:watch": "nodemon --watch backend/src -e ts,js --exec ts-node --project backend/tsconfig.backend.json backend/src/server.ts",
    "backend:build": "tsc -p backend/tsconfig.backend.json",
    "backend:start": "node backend/dist/server.js",
    "seed:admin": "ts-node --transpile-only scripts/seed-admin-backend.ts --email admin@example.com --password ChangeMe123!",
    "gen:hash": "node scripts/gen-hash.js",
    "dev:full": "concurrently \"npm:backend:dev\" \"next dev --turbopack\"",
    "ensure:admin": "ts-node --transpile-only scripts/ensure-admin.ts --email admin@bhesi.com --password ChangeMe123!"
  },
  "prisma-fix": {
    "ensure:users": "ts-node --transpile-only scripts/ensure-users-table.ts --email admin@bhesi.com --password ChangeMe123!"
  },
  "auth-migration": {
    "migrate:admins": "ts-node --transpile-only scripts/migrate-admins-to-users.ts"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^6.16.2",
    "@reduxjs/toolkit": "^2.3.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.20",
    "@types/uuid": "^11.0.0",
    "@types/validator": "^13.15.3",
    "axios": "^1.7.7",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.21.1",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.544.0",
    "morgan": "^1.10.0",
    "next": "15.5.3",
    "nodemon": "^3.1.10",
    "pg": "^8.16.3",
    "pino": "^9.10.0",
    "pino-pretty": "^13.1.1",
    "prisma": "^6.16.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-redux": "^9.1.2",
    "redis": "^5.8.2",
    "sanitize-html": "^2.13.0",
    "uuid": "^13.0.0",
    "validator": "^13.15.15",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^24.5.2",
    "@types/pg": "^8.11.10",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/react-redux": "^7.1.34",
    "@types/sanitize-html": "^2.11.0",
    "@types/supertest": "^2.0.16",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "supertest": "^7.1.1",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
